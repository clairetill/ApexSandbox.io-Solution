public String ageGroup(Integer n) {
    if (ageGroup == null || ageGroup < 0) {
        return 'Invalid age';
    } else if ( ageGroup <= 1) {
        return 'Infant';
    } else if (ageGroup >= 2 && ageGroup <= 14){
        return 'Child';
    } else if (ageGroup >= 15 && ageGroup <= 21){
        return 'Youth';
    } else if (ageGroup >= 22){
        return 'Adult';
    } else {
        return 'Unknown';
        }
    }