public String ageGroup(Integer n) {
    if (age == null || age < 0) {
        return 'Invalid age';
    } else if ( age <= 1) {
        return 'Infant';
    } else if (age >= 2 && age <= 14){
        return 'Child';
    } else if (age >= 15 && age <= 21){
        return 'Youth';
    } else if (age >= 22){
        return 'Adult';
    } else {
        return 'Unknown';
        }
    }
}